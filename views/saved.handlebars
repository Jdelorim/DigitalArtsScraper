<section class="articles">
    <h2 class="headers center grey-text">Articles</h2>
    
    
    {{#each article}}
      <div class="checkArticles container z-depth-5" id="checkmesaved" data-target={{this.saveme}}>
        <div class="row">
            <div class="col s12 center headline deep-purple darken-3 white-text">
             <a href={{this.link}} target="_blank"><img class="left responsive-img" src={{this.img}} ></a> 
             
                <h4 class="left-align">{{this.headline}}</h4>
            
            </div>
            <div class="white">
                <span class="black-text">{{this.blurb}}</span>
            </div>
            <div class="buttonHolder">
                <a data-target={{this.id}} class="deleteBtn waves-effect waves-light deep-purple darken-3 btn">Delete Article</a>
                <a href="#modal1" class="waves-effect waves-light deep-purple lighten-1 btn modal-trigger">Article Notes</a>
            </div>
        </div>
    </div>

    <div id="modal1" class="modal">
    <div class="modal-content">
      <h4 class="putHeadline"><bold></bold></h4>
        <input placeholder="Leave Comments" id="comments" type="text" class="validate">
       </div>
    <div class="modal-footer">
      <a href="#!" class="modal-close waves-effect waves-green btn-flat"><bold>close</bold></a>
    </div>
  </div>
  {{/each}}
     
</section>


